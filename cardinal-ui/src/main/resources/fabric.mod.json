{
	"schemaVersion": 1,
	"id": "${cardinalUiId}",
	"version": "${version}",
	"name": "${cardinalUiName}",
	"description": "${cardinalUiDescription}",
	"authors": [
		"${authors}"
	],
	"contributors": [
		"${contributors}"
	],
	"contact": {
		"homepage": "${homepage}",
		"issues": "${issues}",
		"sources": "${sources}"
	},
	"license": "${license}",
	"icon": "assets/${modId}/icon.png",
	"custom": {
		"modmenu": {
			"parent": "${modId}"
		}
	},
	"environment": "client",
	"mixins": [
		"${cardinalUiId}.mixins.json"
	],
	"accessWidener": "switchy-cardinal-ui.accesswidener",
	"depends": {
		"minecraft": ">=${mc}",
		"fabricloader": ">=${fl}",
		"fabric-api": ">=${fapi}",
		"${coreId}": ">=${version}",
		"${clientId}": ">=${version}",
		"${cardinalId}": ">=${version}",
		"${uiId}": ">=${version}"
	},
	"entrypoints": {
		"switchy_client": [
			"folk.sisby.switchy.client.SwitchyCardinalClient"
		]
	}
}

{
	"schemaVersion": 1,
	"id": "${uiId}",
	"version": "${version}",
	"name": "${uiName}",
	"description": "${uiDescription}",
	"authors": [
		"${authors}"
	],
	"contributors": [
		"${contributors}"
	],
	"contact": {
		"homepage": "${homepage}",
		"issues": "${issues}",
		"sources": "${sources}"
	},
	"license": "${license}",
	"icon": "assets/${modId}/icon.png",
	"custom": {
		"modmenu": {
			"parent": "${modId}"
		}
	},
	"environment": "client",
	"depends": {
		"minecraft": ">=${mc}",
		"fabricloader": ">=${fl}",
		"fabric-api": ">=${fapi}",
		"${coreId}": ">=${version}",
		"${clientId}": ">=${version}"
	},
	"suggests": {
		"owo-lib": ">=${owo}"
	},
	"entrypoints": {
		"switchy_client": [
			"folk.sisby.switchy.ui.SwitchyUI"
		]
	}
}

		"minecraft": ">=${mc}",
		"fabricloader": ">=${fl}",
		"fabric-api": ">=${fapi}",
		"${coreId}": "${version}",
		"${clientId}": "${version}",
		"${uiId}": "${version}",
		"${compatId}": "${version}",
		"${compatUiId}": "${version}",
		"${cardinalId}": "${version}",
		"${cardinalUiId}": "${version}",
		"fabric-permissions-api-v0": "*"
	}
